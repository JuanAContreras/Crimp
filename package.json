{
  "name": "Crimp",
  "productName": "Crimp",
  "version": "1.0.0",
  "description": "Mac app for image, gif and document compression and minification",
  "main": "src/main/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": ["compression", "mac", "electron", "jpg", "electron", "minification", "mac app"],
  "author": "ghosh",
  "license": "MIT",
  "config": {
    "forge": {
      "make_targets": {
        "darwin": [
          "zip"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn",
        "icon": "src/assets/crimp.icns"
      },
      "electronInstallerDMG": {
        "name": "Crimp"
      },
      "github_repository": {
        "owner": "Ghosh",
        "name": "Crimp"
      }
    }
  },
  "dependencies": {
    "electron-compile": "^6.4.2",
    "fs-extra": "^4.0.2",
    "imagemin": "^5.3.1",
    "imagemin-gifsicle": "^5.2.0",
    "imagemin-jpegtran": "^5.0.2",
    "imagemin-pngquant": "^5.0.1",
    "imagemin-svgo": "^5.2.2",
    "plur": "^2.1.2",
    "pretty-bytes": "^4.0.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-dropzone": "^4.1.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "styled-components": "^2.1.2"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "electron-prebuilt-compile": "1.7.6"
  }
}
